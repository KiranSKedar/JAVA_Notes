           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />



	<bean class="com.autowiring.annotations.Address" name="address1">
		<property name="street" value="Ppur road">

		</property>

		<property name="city" value="sangola"></property>
	</bean>


	<bean class="com.autowiring.annotations.Emp" name="Emp1" ></bean>
	
	
	
	

</beans>



